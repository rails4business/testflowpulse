<div id="<%= dom_id post %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= post.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= post.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Slug:</strong>
    <%= post.slug %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= post.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Img square url:</strong>
    <%= post.img_square_url %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Img vertical url:</strong>
    <%= post.img_vertical_url %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Img orizontal url:</strong>
    <%= post.img_orizontal_url %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Body:</strong>
   <div class="rt">  
    <%= post.body %>
    </div>
  </div>
  <div>
    <strong class="block font-medium mb-1">Published at:</strong>
    <%= post.published_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Group:</strong>
    <%= post.group_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Meta:</strong>
    <%= post.meta %>
  </div>
</div>
